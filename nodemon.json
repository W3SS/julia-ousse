{
  "restartable": "rs",
  "ignore": [
    ".git",
    "node_modules/**/node_modules",
    "src/app/**/*.spec.ts"
  ],
  "verbose": true,
  "execMap": {
    "js": "node --harmony"
  },
  "watch": [
    "dist/*"
  ],
  "env": {
    "NODE_ENV": "development"
  },
  "ext": "ts js json css scss html"
}
