{
  "name": "julia-ousse-universal",
  "version": "0.0.4",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ts-node server.ts",
    "build": "ng build",
    "build:prod": "ng build --prod --build-optimizer && npm run build:server",
    "build:server": "ng build --prod --build-optimizer --app 1",
    "develop": "nodemon --exec 'ts-node' server.ts",
    "predocker:build": "npm run build:prod",
    "docker:build": "docker build . -t mikesaprykin/julia-ousse",
    "docker:push": "docker push mikesaprykin/julia-ousse",
    "bump:minor": "ts-node ./bump-version/bump.ts minor",
    "bump:major": "ts-node ./bump-version/bump.ts major",
    "bump:patch": "ts-node ./bump-version/bump.ts patch",
    "test": "ng test --single-run --browser PhantomJS --code-coverage",
    "tdd": "ng test --browser PhantomJS",
    "dev": "ng serve",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "engines": {
    "node": "8.0.0",
    "npm": "5.4.2"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^4.4.4",
    "@angular/common": "^4.4.4",
    "@angular/compiler": "^4.4.4",
    "@angular/core": "^4.4.4",
    "@angular/forms": "^4.4.4",
    "@angular/http": "^4.4.4",
    "@angular/platform-browser": "^4.4.4",
    "@angular/platform-browser-dynamic": "^4.4.4",
    "@angular/platform-server": "^4.4.4",
    "@angular/router": "^4.4.4",
    "@types/node": "^8.0.31",
    "core-js": "^2.5.1",
    "express": "^4.16.0",
    "rxjs": "^5.4.3",
    "save": "^2.3.1",
    "ts-node": "^3.3.0",
    "typescript": "^2.5.3",
    "zone.js": "^0.8.18"
  },
  "devDependencies": {
    "@angular/cli": "^1.4.4",
    "@angular/compiler-cli": "^4.4.4",
    "@angular/language-service": "^4.4.4",
    "@ngrx/core": "^1.2.0",
    "@ngrx/effects": "^4.0.5",
    "@ngrx/router-store": "^4.0.4",
    "@ngrx/store": "^4.0.3",
    "@ngrx/store-devtools": "^4.0.0",
    "@types/jasmine": "^2.5.54",
    "@types/jasminewd2": "^2.0.3",
    "@types/ramda": "^0.24.13",
    "codecov": "^2.3.0",
    "codelyzer": "^3.2.0",
    "jasmine-core": "^2.8.0",
    "jasmine-marbles": "^0.1.0",
    "jasmine-spec-reporter": "^4.2.1",
    "karma": "^1.7.1",
    "karma-chrome-launcher": "^2.2.0",
    "karma-cli": "^1.0.1",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "^1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "karma-phantomjs-launcher": "^1.0.4",
    "karma-viewport": "^0.4.2",
    "nodemon": "^1.12.1",
    "phantomjs-polyfill-includes": "^1.0.2",
    "phantomjs-prebuilt": "^2.1.15",
    "protractor": "^5.1.2",
    "tslint": "^5.7.0"
  }
}
